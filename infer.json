{
  "pipeline": {
    "name": "inference"
  },
  "transform": {
    "image": "infer_tree:latest",
    "cmd": [ 
	"julia",
	"/infer.jl", 
	"/pfs/model/model.jld", 
	"/pfs/attributes/", 
	"/pfs/out/"
    ]
  },
  "parallelism_spec": {
    "constant": "5"
  },
  "input": {
    "cross": [
      {
        "pfs": {
	  "repo": "attributes",
	  "glob": "/*"
	}
      },
      {
	"pfs": {
	  "repo": "model",
	  "glob": "/"
	}
      }
    ]
  }
}
